<?xml version="1.0" encoding="utf-8" ?>
<Rules>
  <Rule Name="MtimeChannel">
    <WebSite>
      <!--<Item Type="ReplaceContent" dir="" file="Web.config" find="" replace="" />-->
      <Item Type="EditConfig" dir="E:\Publish\WebSite\service.channel.mtime.com\" file="Web.config" xpath="/configuration/appSettings/add[@key=&quot;StaticResourceServersVersion&quot;]" attribute="value" value="$MtimeMovieCommunityRoot" />
    </WebSite>
    <Service></Service>
    <Tool>
      <Item Type="EditConfig" dir="E:\Publish\Tool\Channel.GeneratorApp\" file="Channel.GeneratorApp.exe.config" xpath="/configuration/appSettings/add[@key=&quot;StaticResourceServersVersion&quot;]" attribute="value" value="$MtimeMovieCommunityRoot" />
      <Item Type="EditConfig" dir="E:\Publish\Tool\Channel.GeneratorApp\" file="Channel.GeneratorApp.exe.config" xpath="/configuration/appSettings/add[@key=&quot;AutoListen&quot;]" attribute="value" value="true" />
      <Item Type="ReplaceContent" dir="E:\Publish\Tool\Channel.GeneratorApp\Config\" file="SiteUrls.Channel.config" find=":1000" replace="" />
      <Item Type="ReplaceContent" dir="E:\Publish\Tool\Channel.GeneratorApp\Config\" file="SiteUrls.Channel.config" find=":1001" replace="" />
      <Item Type="ReplaceContent" dir="E:\Publish\Tool\Channel.GeneratorApp\Config\" file="SiteUrls.config" find=":1000" replace="" />
      <Item Type="ReplaceContent" dir="E:\Publish\Tool\Channel.GeneratorApp\Config\" file="SiteUrls.config" find=":1001" replace="" />
    </Tool>
  </Rule>
  <Rule Name="MtimeMemberCenter">
    <WebSite>
      <!--<Item Type="ReplaceContent" dir="" file="Web.config" find="" replace="" />-->
      <Item Type="ReplaceContent" dir="E:\Publish\WebSite\MemberCenterNew\Config\" file="SiteUrls.MC.config" find=":1000" replace="" />
      <Item Type="ReplaceContent" dir="E:\Publish\WebSite\MemberCenterNew\Config\" file="SiteUrls.MC.config" find=":1001" replace="" />
      <Item Type="EditConfig" dir="E:\Publish\WebSite\MemberCenterNew\" file="web.config" xpath="/configuration/appSettings/add[@key=&quot;StaticResourceServersVersion&quot;]" attribute="value" value="$MtimeMovieCommunityRoot" />
      <Item Type="EditConfig" dir="E:\Publish\WebSite\MemberCenterNew\" file="web.config" xpath="/configuration/appSettings/add[@key=&quot;StaticResourceServers&quot;]" attribute="value" value="http://114.251.245.9:8000/" />
      <Item Type="EditConfig" dir="E:\Publish\WebSite\MemberCenterNew\" file="web.config" xpath="/configuration/appSettings/add[@key=&quot;SubStaticResourceServers&quot;]" attribute="value" value="http://114.251.245.9:8000/my/" />
    </WebSite>
    <Service></Service>
    <Tool>
    </Tool>
  </Rule>
</Rules>
